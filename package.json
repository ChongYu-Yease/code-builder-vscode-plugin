{
    "name": "weidian-code-builder",
    "displayName": "weidian-code-builder",
    "description": "代码生成器的vscode页面插件",
    "version": "0.0.1",
    "license": "MIT",
    "engines": {
        "vscode": "^1.63.0"
    },
    "author": {
        "name": "xuchongyu",
        "email": "18763006837@163.com"
    },
    "publisher": "weidian-code-builder",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.openFormMaker"
    ],
    "main": "./extension.js",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Form设计器配置",
            "properties": {
                "FormMaker.url": {
                    "type": "string",
                    "default": "http://120.92.142.115/?vscode=1",
                    "description": "表单设计器远程URL"
                }
            }
        },
        "commands": [
            {
                "command": "extension.openFormMaker",
                "title": "打开表单设计器"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "extension.openFormMaker",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.63.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "eslint": "^7.19.0",
        "glob": "^7.1.6",
        "mocha": "^8.2.1",
        "typescript": "^4.1.3",
        "vscode-test": "^1.5.0"
    },
    "dependencies": {
        "open": "^8.4.0"
    }
}
